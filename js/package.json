{
  "name": "ipyigv",
  "version": "0.1.0",
  "description": "A Jupyter wrapper for the igv.js library",
  "author": "Project Jupyter",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//ipyigv.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
